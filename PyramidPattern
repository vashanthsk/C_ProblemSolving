#include<stdio.h>
#include<conio.h>
void main()
{
int row,count,n,display_number=1;
clrscr();
printf("Enter the value for n:");
scanf("%d",&n);
for(row=1;row<=n;row++)
{
   for(count=1;count<=n-row;count++)
   {
      printf(" ");
   }
       for(count=1;count<=row;count++)
       {
          printf("%d ",display_number++);
       }
   printf("\n");
}
getch();
}
